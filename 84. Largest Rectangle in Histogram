function largestRectangleArea(heights: number[]): number {
    heights.push(0)  
let stack = [];
let res = 0;
for (let i = 0; i < heights.length; i++) {
	let heightStart = i;
	while (stack.length && stack[stack.length - 1][1] > heights[i]) {

			res = Math.max(res, (i - pos) * height);
			heightStart = pos; 
		}
	stack.push([heightStart, heights[i]]);
}
return res;
    
};
